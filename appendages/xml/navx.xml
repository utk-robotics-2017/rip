<appendage-template>
    <includes>
        <include standard="true">Wire.h</include>
        <include standard="false">NavX.h</include>
    </includes>
    <constructors>
        <constructor type="NavX" variable="navx">
        </constructor>
    </constructors>
    <setup>
      Wire.begin();
    </setup>
    <commands>
        <command id="kGetYaw" name="getYaw" index-num="false">
            <return-value name="rv" type="float"/>
            <code>
                rv = NavX::getYaw();
            </code>
        </command>
        <command id="kGetPitch" name="getPitch" index-num="false">
            <return-value name="rv" type="float"/>
            <code>
                rv = NavX::getPitch();
            </code>
        </command>
        <command id="kGetRoll" name="getRoll" index-num="false">
            <return-value name="rv" type="float"/>
            <code>
                rv = NavX::getRoll();
            </code>
        </command>
        <command id="kGetStatus" name="getStatus" index-num="false">
            <return-value name="rv" type="char"/>
            <code>
                rv = NavX::getStatus();
            </code>
        </command>
    </commands>
</appendage-template>
